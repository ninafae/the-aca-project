								});
								$(act).append(newLink.append(newImage)); 
			   				}
			   			}
			       	});	
			       	if(defaults.random == true) {
						if ( imageArray.in_array(randomiser) ) {
							defaults.limit++;	
						} 
						//strore our image number
						imageArray.push(randomiser);					
					} else {
						imageArray.push(defaults.limit-1);					
					}
					num++; 	       					
				}
